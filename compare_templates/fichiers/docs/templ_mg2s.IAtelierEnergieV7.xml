<obj id="mg2s.IAtelierEnergieV7.{{atelier.nom}}" class-name="mg2s.IAtelierEnergieV7" nom="{{atelier.nom}}">
	  <fv nom="alarmeEtatAdministratif">
	    <obj id="mg2s.IAtelierEnergieV7.{{atelier.nom}}.alarmeEtatAdministratif.mil.IPropertyBasedAlarmObject$mg2s.IAtelierEnergieV7$alarmeEtatAdministratif.Etat administratif" class-name="mil.IPropertyBasedAlarmObject$mg2s.IAtelierEnergieV7$alarmeEtatAdministratif" nom="Etat administratif">
	      <fv nom="alarmAcousticalWarning" val="true" />
	      <fv nom="alarmeActive" val="false" />
	      <fv nom="alarmTimeDelay" val="0" />
	    </obj>
	  </fv>
	  <fv nom="alarmeEtatBatterie">
	    <obj id="mg2s.IAtelierEnergieV7.{{atelier.nom}}.alarmeEtatBatterie.mil.IPropertyBasedAlarmObject$mg2s.IAtelierEnergieV7$alarmeEtatBatterie.Etat Batterie" class-name="mil.IPropertyBasedAlarmObject$mg2s.IAtelierEnergieV7$alarmeEtatBatterie" nom="Etat Batterie">
	      <fv nom="alarmAcousticalWarning" val="true" />
	      <fv nom="alarmeActive" val="false" />
	      <fv nom="alarmTimeDelay" val="0" />
	    </obj>
	  </fv>
	  <fv nom="alarmeEtatDeSortie">
	    <obj id="mg2s.IAtelierEnergieV7.{{atelier.nom}}.alarmeEtatDeSortie.mil.IPropertyBasedAlarmObject$mg2s.IAtelierEnergieV7$alarmeEtatDeSortie.Etat Du Courant De Sortie" class-name="mil.IPropertyBasedAlarmObject$mg2s.IAtelierEnergieV7$alarmeEtatDeSortie" nom="Etat Du Courant De Sortie">
	      <fv nom="alarmAcousticalWarning" val="true" />
	      <fv nom="alarmeActive" val="false" />
	      <fv nom="alarmTimeDelay" val="0" />
	    </obj>
	  </fv>
	  <fv nom="alarmeEtatLigneVie">
	    <obj id="mg2s.IAtelierEnergieV7.{{atelier.nom}}.alarmeEtatLigneVie.mil.IPropertyBasedAlarmObject$mg2s.IAtelierEnergieV7$alarmeEtatLigneVie.Etat ligne de vie" class-name="mil.IPropertyBasedAlarmObject$mg2s.IAtelierEnergieV7$alarmeEtatLigneVie" nom="Etat ligne de vie">
	      <fv nom="alarmAcousticalWarning" val="true" />
	      <fv nom="alarmeActive" val="false" />
	      <fv nom="alarmTimeDelay" val="0" />
	    </obj>
	  </fv>
	  <fv nom="altIpAddressSupervisedHost" val="" />
	  <fv nom="commentaire" val="" />
	  <fv nom="critique" val="false" />
	  <fv nom="critiqueSite" val="false" />
{% if atelier.DetecteurBatterieBasse %}
	  <fv nom="DetecteurBatterieBasse" ref="{{atelier.DetecteurBatterieBasse}}" />
{% endif %}
{% if atelier.DetecteurDefautAE %}
	  <fv nom="DetecteurDefautAE" ref="{{atelier.DetecteurDefautAE}}" />
{% endif %}
{% if atelier.DetecteurSurSecteur %}
	  <fv nom="DetecteurSurSecteur" ref="{{atelier.DetecteurSurSecteur}}" />
{% endif %}
	  <fv nom="etatInit" enum="1" />

	  <fv nom="hostName" val="{{atelier.hostName}}" />
	  <fv nom="infobulle" val="{{atelier.infobulle}}" />
	  <fv nom="intervalLifelineErreur" val="1000" />
	  <fv nom="intervalLifelineOk" val="1000" />
	  <fv nom="IntervalPollingErreur" val="1000" />
	  <fv nom="ipAddressSupervisedHost" val="{{atelier.adrIpSuperv}}" />
	  <fv nom="isSnmpDate" val="false" />
	  <fv nom="isSnmpNumSer" val="true" />
	  <fv nom="logiqueDeComportement" enum="1" />

	  <fv nom="maxTryNumber" val="5" />
	  <fv nom="maxTryNumberLifeLine" val="4" />
	  <fv nom="maxVarbindsPerPduRequest" val="20" />
	  <fv nom="nbrTestAvantBasculeIP" val="2" />
	  <fv nom="nomCourt" val="{{atelier.nomCourt}}" />
{% if atelier.parentLogique1 %}
	      <fv nom="parentLogique1" ref="{{atelier.parentLogique1}}" />
{% endif %}
{% if atelier.parentLogique2 %}
	      <fv nom="parentLogique2" ref="{{atelier.parentLogique2}}" />
{% endif %}
	  <fv nom="parentPhysique" val="{{atelier.localisation}}" />
	  <fv nom="portEthernet">
	    <obj id="mg2s.IAtelierEnergieV7.{{atelier.nom}}.portEthernet.port.IPortNonSupervise$mg2s.IAtelierEnergieV7$portEthernet.port" class-name="port.IPortNonSupervise$mg2s.IAtelierEnergieV7$portEthernet" nom="port">
	      <fv nom="commentaire" val="" />
	      <fv nom="nomCourt" val="Port" />
	    </obj>
	  </fv>
	  <fv nom="portSnmpAffiche" val="161" />
	  <fv nom="portTrapSnmp" val="-1" />
	  <fv nom="tempoLifeline" val="" />
	  <fv nom="timeout" val="1000" />
	  <fv nom="timeoutInventaire" val="" />
	  <fv nom="timeoutLifeLine" val="400" />
	  <fv nom="typeEquipement" val="N/A" />
	  <fv nom="typeInventaire" enum="2" />

	</obj>
	